<h1><%= t('.title') %></h1>
<div id="form-container">
  <%= form_for @song, html: { class: "form-horizontal" } do |f| %>
    <%= render 'form', form: f %>
    <div class="form-actions">
      <%= f.submit t('.update'), class: 'btn btn-primary' %>
      <%= link_to t('.cancel'), root_path, class: 'btn' %>
    </div>
  <% end %>
</div>
